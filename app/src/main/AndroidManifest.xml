<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="piccle.startupwe.piccle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".StartActivity"></activity>
        <activity android:name=".JoinActivity" />
        <activity android:name=".GiveRegisterActivity"></activity>
        <activity android:name=".MakingReservationActivity" />
        <activity android:name=".ConfirmingReservation" />
        <activity android:name=".ReservationConditionSetting" />
        <activity android:name=".GiveMainActivity" />
        <activity android:name=".TakeMypageActivity" />
        <activity android:name=".ShowGiverListActivity"></activity>
        <activity android:name=".PortfolioDetailActivity"></activity>
        <activity android:name=".giverSelectedActivity">
        </activity>
    </application>
</manifest>